worker_processes 1;

events {
    worker_connections 1024;
}

http {
    include /etc/nginx/conf.d/*.conf;
    include /etc/nginx/sites-enabled/*.*;

    server {
            #root /var/www/html;

            listen ${NGINX_PORT};
            server_name localhost 127.0.0.1;

            location /tests {
                location /tests/1 {
                    return 200 'Hello world!!!1';
                }
            }
    }
}
