{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./angularapp",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2020",
    "module": "es2015",
    "lib": [
      "es2020",
      "dom"
    ],
    "useDefineForClassFields": false
  }
}
